<html>
  <head>
    <title>{{title}}</title>
  </head>
  <body>
    <can-import from="cms/styles.less" />
    <can-import from="cms/app" export-as="viewModel" />

    <can-import from="cms/comp-header/">
        {{#isResolved}}
        <comp-header></comp-header>
        {{/isResolved}}
    </can-import>

    <h1>{{message}}</h1>

    {{#switch env.NODE_ENV}}
      {{#case "production"}}
        <script src="{{joinBase 'node_modules/steal/steal.production.js'}}"  main="cms/index.stache!done-autorender"></script>
      {{/case}}
      {{#default}}
        <script src="/node_modules/steal/steal.js"></script>
      {{/default}}
    {{/switch}}
  </body>
</html>
